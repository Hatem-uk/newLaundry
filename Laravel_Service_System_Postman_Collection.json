					"if (pm.response.code === 200) {",
					"    const response = pm.response.json();",
					"    if (response.token) {",
					"        // Determine which token to save based on the request",
					"        const requestName = pm.info.requestName;",
					"        if (requestName.includes('Admin')) {",
					"            pm.environment.set('admin_token', response.token);",
					"            console.log('Admin token saved');",
					"        } else if (requestName.includes('Laundry')) {",
					"            pm.environment.set('laundry_token', response.token);",
					"            console.log('Laundry token saved');",
					"        } else if (requestName.includes('Customer')) {",
					"            pm.environment.set('customer_token', response.token);",
					"            console.log('Customer token saved');",
					"        }",
					"    }",
					"}"
